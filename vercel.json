{
    "version": 2,
    "builds": [
        {
            "src": "build/index.js",
            "use": "@now/node-server"
        },
        {
          "src": "public/*",
          "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "build/index.js"
        }
    ]
}
