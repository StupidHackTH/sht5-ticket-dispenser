{
    "version": 2,
    "builds": [
        {
            "src": "build/index.js",
            "use": "@vercel/node"
        },
        {
          "src": "public/*",
          "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "build/index.js"
        }
    ]
}
