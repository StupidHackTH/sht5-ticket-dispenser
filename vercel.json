{
    "version": 2,
    "builds": [
        {
            "src": "build/index.js",
            "use": "@now/node"
        },
        {
          "src": "public/*",
          "use": "@now/static"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "/"
        }
    ]
}
